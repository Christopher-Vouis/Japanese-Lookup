{
	"name": "Japanese Lookup",
	"description": "Look up highlighted Japanese text on Jisho.org by right clicking",
	"version": "1.0",
	"manifest_version": 3,
	  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["content-script.js"]
    }],
	"background": {
    "service_worker": "background.js"
  },
	"permissions": [
	"contextMenus",
	"tabs",
	"background"
	]
}